{
  "name": "node-express-typescript",
  "description": "node-express-typescript Sample Application (TypeScript)",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-bpm": "node ./out/www.js",
    "compile": "node ./node_modules/typescript/bin/tsc -p ./src",
    "antlr4ts": "antlr4ts -o ./src/models/dynamic_binding  -visitor ./src/models/dynamic_binding/binding_grammar.g4",
    "start-service": "nodemon -r source-map-support/register dist/server/server.js",
    "build": "tsc -p src/server; tsc -p src/client;",
    "bundle": "rollup -c",
    "watch": "npm run build; npm run bundle; concurrently --names 'server,client,rollup' -c 'white.bgBlue,bgCyan,white.bgRed' 'tsc -p ./src/server -w' 'tsc -p ./src/client -w' 'rollup -c -w' ",
    "prepublishOnly": "npm run lint; npm run build",
    "lint": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "antlr4": "^4.7.1",
    "antlr4ts": "^0.4.1-alpha.0",
    "bignumber.js": "^4.0.1",
    "body-parser": "^1.19.0",
    "bpmn-moddle": "^7.0.3",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "ejs": "^2.5.6",
    "express": "^4.17.1",
    "mongoose": "^5.9.21",
    "morgan": "^1.10.0",
    "solc": "^0.6.10",
    "web3": "^1.2.9",
    "colors": "^1.4.0",
    "commander": "latest",
    "dotenv": "latest",
    "kafka-node": "latest",
    "lit-element": "latest",
    "ws": "latest"
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.0",
    "@types/body-parser": "^1.19.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/gulp": "^4.0.6",
    "@types/gulp-less": "^0.0.31",
    "@types/gulp-nodemon": "^0.0.32",
    "@types/marked": "^1.1.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^12.7.2",
    "antlr4ts-cli": "^0.4.0-alpha.4",
    "browser-sync": "^2.26.7",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "gulp-nodemon": "^2.5.0",
    "gulp-tsb": "^4.0.5",
    "marked": "^1.1.0",
    "typescript": "^3.9.6",
    "xml2js": "^0.4.23",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "concurrently": "latest",
    "eslint": "latest",
    "nodemon": "^1.18.11",
    "rollup": "latest",
    "rollup-plugin-node-resolve": "latest",
    "source-map-support": "^0.5.12"
  }
}
